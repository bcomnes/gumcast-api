{
  "name": "gumcast-api",
  "profile": "gumcast",
  "regions": ["us-east-2"],
  "environment": {
    "NODE_ENV": "development"
  },
  "proxy": {
    "command": "node api/bin.js --config ./config.json"
  },
  "stages": {
    "production": {
      "domain": "api.gumcast.com"
    },
    "staging": {
      "domain": "api-staging.gumcast.com"
    }
  }
}
